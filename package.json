{
  "name": "kagrenac",
  "version": "0.1.0",
  "author": "ESOUI Community",
  "bin": {
    "kag": "./bin/run",
    "kagrenac": "./bin/run"
  },
  "bugs": "https://github.com/esoui/kagrenac/issues",
  "dependencies": {
    "@oclif/command": "^1.5.12",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/yeoman-generator": "^3.0.2",
    "change-case": "^3.1.0",
    "enquirer": "^2.3.0",
    "os": "^0.1.1",
    "tslib": "^1.9.3",
    "yeoman-environment": "^2.3.4",
    "yeoman-generator": "^3.2.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13.3.0",
    "pkg": "^4.3.7",
    "rimraf": "^2.6.3",
    "ts-node": "^8",
    "tslint": "^5",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "/oclif.manifest.json",
    "/bin",
    "/lib",
    "/templates"
  ],
  "homepage": "https://github.com/esoui/kagrenac",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "kag",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "esoui/kagrenac",
  "scripts": {
    "version": "npx oclif-dev readme && sed -ri'' 's/^## `/### `/g' README.md && git add README.md",
    "lint": "npx tslint -p . -t stylish",
    "pretest": "yarn run lint",
    "test": "npx nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "prepack": "rm -rf lib && npx tsc -b && npx oclif-dev manifest",
    "pack": "npx oclif-dev pack",
    "postpack": "rm -f oclif.manifest.json"
  },
  "types": "lib/index.d.ts"
}
